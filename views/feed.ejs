  <script>
    // $(".grid").imagesLoaded(function () {
    //   $(".grid").masonry({
    //     itemSelector: ".grid-item",
    //   });
    // });
    // Function to initialize Masonry layout after the first row of images is loaded

    function initializeMasonry() {
      $(".grid").masonry({
        itemSelector: ".grid-item",
      });
    }

    // Wait for the first row of images to load and call initializeMasonry
    let imagesLoaded = 0;
    const imagesInFirstRow = 4; // Assuming 3 images per row for demonstration

    $(".grid img").on("load", function () {
      imagesLoaded++;
      if (imagesLoaded >= imagesInFirstRow) {
        initializeMasonry();
      }
    });

    // In case some images are cached and already loaded before the script runs
    $(".grid img").each(function () {
      if (this.complete) {
        imagesLoaded++;
      }
    });

    // Check if the first row is loaded immediately after the script runs
    if (imagesLoaded >= imagesInFirstRow) {
      initializeMasonry();
    }
  </script>
